            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "description": "The adhan file id to delete.",
            "in": "path",
            "required": true,
            "type": "string",
            "pattern": "adhanFiles/[^/]+"
          }
        ],
        "tags": [
          "AdhanService"
        ]
      }
    }
  },
